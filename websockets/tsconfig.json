{
  "display": "Node LTS",
  "_version": "20.1.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "Preserve",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    /* Additional Options */
    "baseUrl": ".",
  },
}